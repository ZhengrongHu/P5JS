//Rectangles changing color and move
var offset = 0;

function setup() {
  createCanvas(800,800);
}

function draw() {
  offset = offset +1;
  
  background(255);
  strokeWeight(6);
  stroke(0);
  
  for (var x = 0; x <= width; x += 50){
    fill (random(255), random(150), random(255));
    rect(x + offset, x+1, 25, 25);
    
  }
}
